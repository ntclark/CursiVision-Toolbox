EXSTYLE WS_EX_CONTROLPARENT
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN

   LTEXT "After each signature is gathered, move the signing area",-1,16,16, 128 + 64, 12
   EDITTEXT IDDI_DISPOSITION_MULTI_SIGN_RIGHT_MOTION,16 + 16, 16 + 12, 40, 12, ES_CENTER
   LTEXT "inches to the right, and",-1,16 + 16 + 40 + 4, 16 + 12 + 4, 128, 12
   EDITTEXT IDDI_DISPOSITION_MULTI_SIGN_DOWN_MOTION,16 + 16, 16 + 12 + 12 + 4, 40, 12, ES_CENTER
   LTEXT "inches down in the document",-1,16 + 16 + 40 + 4, 16 + 12 + 4 + 12 + 4, 128, 12

   LTEXT "When",-1,16, 16 + 12 + 4 + 12 + 4 + 16, 24, 12
   EDITTEXT IDDI_DISPOSITION_SAVE_AFTER_SIGN_COUNT,16 + 16 + 6 + 4,16 + 12 + 4 + 12 + 4 + 16 - 4,20,12,ES_CENTER | ES_NUMBER
   LTEXT "signatures have been gathered, process the file as indicated",-1,16 + 16 + 4 + 20 + 6 + 10 + 4,16 + 12 + 4 + 12 + 4 + 16,128 + 64,24
   LTEXT "in the disposition settings",-1,16,16 + 12 + 4 + 12 + 4 + 16 + 12,128 + 64,24

   LTEXT "Use these values to collect a list of signatures on a single document over \r\
a period of time.",-1,16,16 + 12 + 4 + 12 + 4 + 16 + 12 + 32,256, 128
   LTEXT "For example, to fill in a sign-in sheet, specify as many signatures as you have\r\
